# Zeitwork Edge Proxy Configuration
# Copy this file to /etc/zeitwork/edge-proxy.env and update values

# Service configuration
PORT=443
LOG_LEVEL=info
ENVIRONMENT=production

# Load balancer connection
LOAD_BALANCER_URL=http://localhost:8082

# SSL/TLS configuration (optional - for HTTPS)
# Leave empty for HTTP-only mode
SSL_CERT_PATH=
SSL_KEY_PATH=

# Rate limiting (requests per second per IP)
RATE_LIMIT_RPS=100
